<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.learnnavi.app"
	android:versionCode="170" android:versionName="0.8.0">

	<uses-sdk 
		android:minSdkVersion="10" 
		android:targetSdkVersion="13">
	</uses-sdk>

	<uses-permission 
		android:name="android.permission.INTERNET">
	</uses-permission>
	
	<application 
		android:icon="@drawable/icon" 
		android:label="@string/app_name"
		android:theme="@style/ThemeSelector"
		android:allowBackup="true">
        
		<activity 
			android:name="Kelutral"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		
		<activity 
			android:name="Dictionary"
			android:launchMode="singleTop" 
			android:exported="false">
			<intent-filter>
				<action android:name="android.intent.action.SEARCH"/>
			</intent-filter>
			<meta-data 
				android:resource="@xml/searchable"
				android:name="android.app.searchable"/>
		</activity>
		
		<activity 
			android:name="Preferences"
			android:exported="false">
		</activity>	
		<!--
		<activity 
			android:name="HorenBrowse" 
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>		
		-->
		<provider
			android:name="NaviWords"
			android:authorities="org.learnnavi.mobile.naviword">
		</provider>
		
	</application>

</manifest>
